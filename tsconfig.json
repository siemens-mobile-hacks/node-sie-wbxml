{
	"compilerOptions": {
		"declaration": true,
		"rootDir": ".",
		"outDir": "./dist",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"]
	},
    "include": ["src"]
}
